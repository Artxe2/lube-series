/*
VSCE: ESLint
VSCE: Sort JS object keys
VSCE: JavaScript and TypeScript Nightly
*/
{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": ["plugin:lube/recommended"],
	"globals": {
		"RequestInit": true
	},
	"ignorePatterns": ["**/types/**/*.d.ts", "**/public.d.ts", "**/private.d.ts"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["lube"],
	"rules": {}
}